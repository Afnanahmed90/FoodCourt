<div class="pt-4 mb-8 center">
  <%= simple_form_for  @branch, url: restaurant_branch_path(@restaurant, @branch), method: :put do |f| %>
    <div class="mb-6 flex">
      <%= f.input :city %>
    </div>
    <div class="mb-6 flex">
      <%= f.input :address %>
    </div>
    <div class="btn rounded">
      <%= f.submit 'submit' %>
    </div>
  <% end %>
</div>
<hr class="border mt-6"/>
